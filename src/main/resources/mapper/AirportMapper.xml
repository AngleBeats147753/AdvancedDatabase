<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.backend.dao.AirportDao">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.example.backend.pojo.AirportDO">
        <id column="id" property="id" />
        <result column="airport_id_code" property="airportIdCode" />
        <result column="airport_code" property="airportCode" />
        <result column="city_id" property="cityId" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, airport_id_code, airport_code, city_id
    </sql>

</mapper>
